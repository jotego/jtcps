            <part name="prl1.u196" crc="65c28bc9" map="01"/>
            <part name="prh2.u222" crc="fff85f9b" map="10"/>
        </interleave>
        <!-- audiocpu -->
        <part name="sound.u191" crc="a4823a1b"/>
