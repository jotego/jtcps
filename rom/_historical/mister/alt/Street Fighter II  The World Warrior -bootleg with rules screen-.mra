        <!-- CPS-B config for sf2rules --> 
        <part> FF FF FF FF FF FF 14 12 10 0E 0C 36 00 0A 0E 0E 0E 30 </part>
        <!-- Mapper for  --> 
        <part> 1F 40 C8 73 F7 </part>
        <part> 08 </part>
